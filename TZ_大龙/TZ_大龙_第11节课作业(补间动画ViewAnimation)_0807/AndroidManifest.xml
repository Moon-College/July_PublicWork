<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tz.lsn11_animation"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tz.lsn11_animation.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tz.lsn11_animation.AnimationActivity"></activity>
        <activity android:name="com.tz.lsn11_animation.AlphaAnimationActivity"></activity>
        <activity android:name="com.tz.lsn11_animation.ScaleAnimationActivity"></activity>
        <activity android:name="com.tz.lsn11_animation.TranslateAnimationActivity"></activity>
        <activity android:name="com.tz.lsn11_animation.RotateAnimationActivity"></activity>
        <activity android:name="com.tz.lsn11_animation.SetAnimationActivity"></activity>
    </application>

</manifest>
