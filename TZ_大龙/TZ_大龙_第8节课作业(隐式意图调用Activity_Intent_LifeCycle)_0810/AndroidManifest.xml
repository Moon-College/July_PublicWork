            android:name="com.tz.lsn8_acivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tz.lsn8_acivity.SecondActivity" />
        <activity android:name="com.tz.lsn8_acivity.PhotoActivity" />
        <activity 
            android:name="com.tz.lsn8_acivity.BaseActivity" >
            <intent-filter>
                <action android:name="ACTION_ABC" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
